{
  "rules": {
    "semi": "off",
    "arrow-parens": "off",
    "function-paren-newline": "off",
    "object-curly-newline": "off",
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "next"
      }
    ],
    "no-use-before-define": [
      "error",
      {
        "variables": false
      }
    ],
    "no-multi-str": 0,
    "linebreak-style": ["error", "windows"]
  },
  "env": {
    "node": true,
    "mocha": true
  },
  "parserOptions": {
    "ecmaVersion": 8
  },
  "parser": "babel-eslint",
  "extends": ["airbnb-base"]
}
